<h1>Hello, Rails!</h1>

<%=  form_tag(articles_path, method: "get") do %>
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q, params[:q]) %>
  <%= submit_tag("Search") %>
<% end %>

<div>
  <% @articles.each do |article|%>


    <article>
      <div class="row article" data-article-id="<%=article.id %>">
        <div class="col-sm-6 col-md-4">
          <figure>
            <img src="data:image/jpeg;base64,">
          </figure>
        </div>
        <div class="col-sm-6 col-md-8">
          <span class="label label-default pull-right"><i class="glyphicon glyphicon-comment"></i><%=article.comments.size %></span>
          <div class="dropup ">
            <button class="btn btn-default btn-group-xs dropdown-toggle" type="button" id="dropdownMenu2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              Dropup
              <span class="caret"></span>
            </button>
            <ul class="dropdown-menu" aria-labelledby="dropdownMenu2">
              <li> <%= link_to 'Rate', new_article_rating_path(article), class: 'btn btn-default'%></li>

              <% if policy(article).edit? %>
                <li><%= link_to 'Edit', edit_article_path(article), class: 'btn btn-warning' %></li>
              <% end %>

              <li><%= link_to 'Show', article_path(article), class: 'btn btn-info' %></li>

              <% if policy(article).destroy? %>
                <li><%= link_to 'Destroy', 'javascript:void(0)', class: 'btn btn-danger remove-article-btn' %></li>
              <% end %>
            </ul>
          </div>
          <h4>
            <%= link_to article.title, article_path(article) %>
          </h4>
          <p><%= article.text %></p>
          <section>

            <i class="glyphicon glyphicon-user"></i> <%=article.user.first_name %>
            <i class="glyphicon glyphicon-calendar"> </i><%=  l article.created_at , format: :short%>
            <i class="glyphicon glyphicon-eye-open"></i> <%=article.show %>
            <%= link_to 'More ...', article_path(article), class: 'btn btn-default btn-sm pull-right' %>
          </section>
        </div>
      </div>
    </article>


  <% end %>
  <%= paginate @articles %>
</div>


<% @list_ip.each do |ip, articles| %>
  <tr>
    <td>
      <%= ip %>
    </td>
    <td>
      <% articles.each do |article|   %>
        <p>
          <%= article.user.email %>
        </p>
      <% end %>
    </td>
  </tr>

<% end %>